{
  "name": "social-activities-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "yarn generate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "prettier:write": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "prisma:format": "prisma format",
    "format": "yarn prettier:write && yarn prisma:format",
    "prepare": "husky install",
    "generate:prisma": "prisma generate",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/pages/api",
    "generate": "yarn generate:prisma && yarn generate:nexus"
  },
  "dependencies": {
    "@apollo/client": "3.11.8",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "4.16.2",
    "apollo-server-micro": "3.13.0",
    "framer-motion": "10.18.0",
    "graphql-scalars": "1.23.0",
    "micro": "10.0.1",
    "micro-cors": "0.1.1",
    "next": "13.5.6",
    "next-auth": "4.24.7",
    "nexus": "1.3.0",
    "nodemailer": "6.9.15",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "4.12.0"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/micro-cors": "0.1.3",
    "@types/node": "18.19.50",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.13",
    "eslint-config-prettier": "9.1.0",
    "husky": "8.0.3",
    "prettier": "3.3.3",
    "prisma": "4.16.2",
    "ts-node": "10.9.2",
    "typescript": "4.9.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
