{
  "name": "social-activities-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "yarn generate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "prettier:write": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "prisma:format": "prisma format",
    "format": "yarn prettier:write && yarn prisma:format",
    "prepare": "husky install",
    "generate:prisma": "prisma generate",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api",
    "generate": "yarn generate:prisma && yarn generate:nexus"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@prisma/client": "^4.0.0",
    "apollo-server-micro": "3.10.0",
    "graphql-scalars": "^1.17.0",
    "micro": "9.4.0",
    "micro-cors": "^0.1.1",
    "next": "12.2.2",
    "nexus": "^1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "18.0.0",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "prisma": "^4.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
