{
  "name": "social-activities-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "yarn generate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "prettier:write": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "prisma:format": "prisma format",
    "format": "yarn prettier:write && yarn prisma:format",
    "prepare": "husky install",
    "generate:prisma": "prisma generate",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/pages/api",
    "generate": "yarn generate:prisma && yarn generate:nexus"
  },
  "dependencies": {
    "@apollo/client": "3.7.0",
    "@chakra-ui/react": "2.3.6",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@next-auth/prisma-adapter": "1.0.5",
    "@prisma/client": "4.5.0",
    "apollo-server-micro": "3.10.3",
    "framer-motion": "7.6.1",
    "graphql-scalars": "1.20.0",
    "micro": "9.4.1",
    "micro-cors": "0.1.1",
    "next": "12.2.5",
    "next-auth": "4.14.0",
    "nexus": "1.3.0",
    "nodemailer": "6.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.6.0"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/micro-cors": "0.1.2",
    "@types/node": "18.0.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "eslint": "8.26.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "prisma": "4.5.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
