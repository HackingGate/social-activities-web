{
  "name": "social-activities-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "yarn generate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "prettier:write": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "prisma:format": "prisma format",
    "format": "yarn prettier:write && yarn prisma:format",
    "prepare": "husky install",
    "generate:prisma": "prisma generate",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/pages/api",
    "generate": "yarn generate:prisma && yarn generate:nexus"
  },
  "dependencies": {
    "@apollo/client": "3.8.1",
    "@chakra-ui/react": "2.8.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "4.16.2",
    "apollo-server-micro": "3.12.0",
    "framer-motion": "10.15.1",
    "graphql-scalars": "1.22.2",
    "micro": "10.0.1",
    "micro-cors": "0.1.1",
    "next": "13.4.13",
    "next-auth": "4.23.0",
    "nexus": "1.3.0",
    "nodemailer": "6.9.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.10.1"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/micro-cors": "0.1.3",
    "@types/node": "18.17.5",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.13",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "prettier": "3.0.1",
    "prisma": "4.16.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
